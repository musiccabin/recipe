<h1>Sign In</h1>

<%= form_with url: session_path, local: true do |f| %>
<div>
    <%= f.label :email %>
    <%= f.email_field :email %>
</div>
<div>
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>
<%= f.submit "Sign In"%>
<%= link_to "Sign Up", new_user_path%>
<% end %>
 <%= link_to "Oops, I lost my password! @.@", password_reset_path%>
